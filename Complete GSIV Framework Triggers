						<script>local line = getCurrentLine()

for id, name, time in line:gmatch("&lt;progressBar id='([^']+)' value='[^']+' text=\"([^\"]+)\" left='[^']+' top='[^']+' width='[^']+' height='[^']+' time='([^']+)'/&gt;") do
  activeDebuffs[id] = {
    name = name,
    time = time
  }
  echo(string.format("Captured debuff: ID=%s, Name=%s, Time=%s\n", id, name, time))
end

