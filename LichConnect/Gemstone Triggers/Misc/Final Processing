			<name>Final Processing.</name>
			<script>prevLine = line

replaceAll("&amp;gt;", "&gt;")
replaceAll("&amp;lt;", "&lt;")
replaceAll("&amp;quot;","\"")
replaceAll("&amp;apos;","'")

for tag in line:gmatch("&lt;[^&gt;]+&gt;") do
  selectString(tag, 1)
  replace("")
end

deselect()
resetFormat()

</script>
